<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <Authors>Katlego Mokhethi</Authors>
    <Company>Katlego Mokhethi</Company>
    <Product>SeresProducoes Website</Product>
    <Description>Re-branding of Seres Producoes record label's website</Description>
    <PackageProjectUrl>https://seresproducoes.azurewebsites.net/</PackageProjectUrl>
    <PackageIcon>Logo.png</PackageIcon>
    <PackageIconUrl />
    <PackageTags>ASPNETCORE WebApplication</PackageTags>
    <PackageReleaseNotes>New Seres Producoes website | Developed by Katlego Mokhethi [mokhethikatlego@gmail.com]</PackageReleaseNotes>
